    "discord.js-light": "^4.5.1",
    "ejs": "^3.1.6",
    "erela.js": "^2.3.3",
    "erela.js-apple": "^1.2.6",
    "erela.js-deezer": "^1.0.7",
    "erela.js-facebook": "^1.0.4",
    "erela.js-filter": "^0.2.3",
    "express": "^4.17.2",
    "jquery": "^3.6.0",
    "lavasfy": "^2.3.0",
    "lodash": "^4.17.21",
    "moment-duration-format": "^2.3.2",
